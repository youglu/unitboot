{
  "name": "kmf-client",
  "version": "1.1.0",
  "description": "test electron with better-sqlite",
  "main": "src/index.js",
  "scripts": {
    "compile": "tsc && copyfiles -f src/index.html  && copyfiles -f src/axios.min.js build",
    "start": "electron-forge start",
    "prod": "set NODE_ENV=prod& chcp 65001 && electron ./src/index ",
    "dev": "set NODE_ENV=development& chcp 65001 && electron ./src/index ",
    "dev-linux": "electron ./src/index",
    "package": "electron-forge package --platform=win32 --arch=ia32",
    "package-mac": "electron-forge package --platform=darwin --arch=x64",
    "build-icon": "electron-icon-builder --input=./src/icon/kmf-256.png --output=build --flatten",
    "pack": "set NODE_ENV=prod & electron-builder --dir",
    "dist": "set NODE_ENV=prod & electron-builder",
    "make": "electron-forge make --platform=win32 --arch=ia32",
    "make-mac": "electron-forge make --platform=darwin --arch=x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "kmf"
  ],
  "author": "JP QUEAU",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/union/kmf-client/issues"
  },
  "homepage": "https://github.com/union/kmf-client#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "cache-base": "^4.0.2",
    "electron-log": "^5.0.0-beta.25",
    "electron-updater": "^6.1.4",
    "element-ui": "^2.15.13",
    "js-cookie": "^3.0.5",
    "jsencrypt": "^3.3.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-cors": "^0.0.16",
    "koa-router": "^12.0.0",
    "map-cache": "^0.2.2",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "nodemon": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "portscanner": "^2.2.0",
    "sequelize": "^6.32.1",
    "snowflake-id-js": "^1.0.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.1.3",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.2.1",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "copyfiles": "^2.4.1",
    "electron": "^22.0.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-builder": "^24.6.4",
    "rimraf": "^5.0.1"
  },
  "build": {
    "productName": "康美风中间件",
    "appId": "com.union.kmfmc",
    "copyright": "union",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "src/icon/icon.ico",
      "uninstallerIcon": "src/icon/icon.ico",
      "installerHeaderIcon": "src/icon/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "康美风中间件"
    },
    "extraResources": [
      {
        "from": "src/db",
        "to": "./db"
      }
    ],
    "win": {
      "icon": "src/icon/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    }
  }
}
